  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/date-utils.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/date-utils.json"
      }
    ]
  },
  "devDependencies": {
    "vsce": "^1.75.0"
  }
